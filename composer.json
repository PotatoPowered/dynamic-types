{
  "name": "potatopowered/dynamic-types",
  "description": "The dynamic-types repository will be more of a library that allows other Potato Powered applications to dynamically lookup and identify entities based on their table name.",
  "type": "cakephp-plugin",
  "keywords": [
    "dynamic-types",
    "CakePHP",
    "Potato Powered Software"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Blake Sutton",
      "email": "sutton.blake@gmail.com",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "~3.0",
    "cakephp/migrations": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "cakephp/cakephp-codesniffer": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "DynamicTypes\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DynamicTypes\\Test\\": "tests"
    }
  },
  "scripts": {
    "check": [
      "@test",
      "@cs-pps"
    ],
    "cs-check": "vendor/bin/phpcs --colors -p --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./src ./tests",
    "cs-fix": "vendor/bin/phpcbf --colors --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./src ./tests",
    "cs-pps": "vendor/bin/phpcs -p --extensions=php --standard=vendor/cakephp/cakephp-codesniffer/CakePHP ./src ./tests ./config",
    "test": "vendor/bin/phpunit --colors=always"
  }
}
